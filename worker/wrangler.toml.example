[triggers]
crons = ["*/5 * * * *"]

[vars]
TRANSLINK_BASE_URL = "https://gtfsapi.translink.ca/v3/gtfsrealtime"
TRANSLINK_API_KEY = "your-api-key"
DATABASE_URL = "your-database-url"
